{% extends "base.twig" %}

{% block content %}
  <article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
    {% for item in post.meta('content_blocks') %}
      {% include 'content-blocks/' ~ item.acf_fc_layout|replace({'_': '-'}) ~ '.twig' %}
    {% endfor %}
  </article>
{% endblock %}